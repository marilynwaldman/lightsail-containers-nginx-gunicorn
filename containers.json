{
    "proxy": {
        "image": ":sample-service.nginx-container.22",
        "ports": {
            "80": "HTTP"
        },
        "environment": {
            "NGINX_ENVSUBST_OUTPUT_DIR": "/etc/nginx",
            "FLASK_HOST": "localhost",
            "FLASK_PORT": "5000"
        }
    },
    "flask": {
        "image": ":sample-service.flask-container.21",
        "command": ["flask", "run", "--host","0.0.0.0"],
        "ports": {
            "5000": "HTTP"
        },
        "environment": {
            "FLASK_APP": "app.py"
        }


    }
}

