{
    "proxy": {
        "image": ":sample-service.nginx-container.22",
        "ports": {
            "80": "HTTP"
        },
        "environment": {
            "NGINX_ENVSUBST_OUTPUT_DIR": "/etc/nginx"
        }
    },
    "flask": {
        "image": ":sample-service.flask-container.21",
        "command": ["gunicorn", "-w", "1", "-b", "0.0.0.0:8000", "wsgi:server"],
        "ports": {
            "8000": "HTTP"
        },
        "environment": {
            "FLASK_APP": "app.py"
        }


    }
}

