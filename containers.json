{
    "proxy": {
        "image": ":sample-service.nginx-container.27",
        "ports": {
            "80": "HTTP"
        },
        "environment": {
            "NGINX_ENVSUBST_OUTPUT_DIR": "/etc/nginx",
            "FLASK_HOST": "localhost",
            "FLASK_PORT": "8000"
        }
    },
    "flask": {
        "image": ":sample-service.flask-container.26",
        "command": ["gunicorn", "-w", "1", "-b", "0.0.0.0:8000", "wsgi:server"],
        "ports": {
            "8000": "HTTP"
        },
        "environment": {
            "FLASK_APP": "app.py"
        }


    }
}

